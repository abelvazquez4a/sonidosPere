<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activitySonido"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/templetime"
    tools:context=".Sonido">

    <ImageView
        android:id="@+id/imageView_song"
        android:layout_width="279dp"
        android:layout_height="294dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="30dp"
        android:onClick="cancion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.823"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.351"
        tools:src="@drawable/frame1" />

    <ImageView
        android:id="@+id/imageView_link"
        android:layout_width="107dp"
        android:layout_height="103dp"
        android:onClick="tono1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_ataqueCircular"
        android:layout_width="171dp"
        android:layout_height="175dp"
        android:layout_marginTop="144dp"
        android:onClick="tono2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.983"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.55"
        tools:src="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>